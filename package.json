{
  "name": "bot-ts",
  "version": "1.0.0",
  "private": true,
  "main": "dist/ts/main.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "xrun": "ts-node-esm --project tsconfig.json src/main.ts",
    "devi": "esrun --watch src/main.ts",
    "devv": "node --loader ts-node/node-hooks src/main.ts",
    "serve": "node dist/ts/main.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "dev": "ts-node-esm -r tsconfig-paths/register --project=tsconfig.json  --experimental-specifier-resolution=node ./src/main.ts"
  },
  "dependencies": {
    "@discordjs/voice": "^0.11.0",
    "@discordx/importer": "^1.1.10",
    "@discordx/music": "^4.1.0",
    "@types/mustache": "^4.2.1",
    "btoa": "^1.2.1",
    "cors": "^2.8.5",
    "discord-music-player": "^9.0.2",
    "discord.js": "^14.0.3",
    "discordx": "^11.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.0.2",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.0.1",
    "mongodb": "^4.8.1",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.1",
    "node-fetch": "^3.2.9",
    "reflect-metadata": "^0.1.13",
    "sha256": "^0.2.0",
    "socket.io": "^4.5.1",
    "socket.io-stream": "^0.9.1",
    "spdl-core": "^2.0.2",
    "youtube-playlist": "^1.0.4",
    "ytdl-core": "^4.11.0",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@types/btoa": "^1.2.3",
    "@types/express": "4",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^3.0.0",
    "@types/node": "^18.0.6",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
